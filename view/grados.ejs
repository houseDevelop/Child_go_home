          <p class="card-text">SECUNDARIA</p>
          <div class="d-flex justify-content-between align-items-center">
            <div class="btn-group">
              <a href="/regsalon" class="btn btn-primary btn-block"><samp
                class="glyphicon glyphicon-shopping-cart"> </samp> Agregar Salon
              </a>
            </div>
          <div>
            <a href="/listasalon" class="btn btn-warning">Ver Salones</a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- Segundo contenedor -->
  <div class="col">
    <div class="card shadow-sm">
      <img src="images/fliyers/37.png" class="d-block w-100">
